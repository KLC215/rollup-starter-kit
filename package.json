{
  "name": "YourLibraryName",
  "version": "1.0.0",
  "main": "dist/bundle.common.js",
  "module": "dist/bundle.esm.js",
  "unpkg": "dist/bundle.min.js",
  "keywords": [
    "rollup",
    "babel",
    "starter"
  ],
  "author": "Leo Hui <leohxj@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "dev": "cross-env NODE_ENV=development rollup -w -c rollup.config.dev.js",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c rollup.config.prod.js",
    "test": "npm run flow && jest",
    "flow": "flow check",
    "cz": "git-cz"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "files": [
    "src",
    "dist/*.js"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.62.0",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.3",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.3.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
